{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/cron-trigger",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron-trigger",
      "schedule": "0 13 * * *"
    },
    {
      "path": "/api/cron-trigger",
      "schedule": "0 17 * * *"
    },
    {
      "path": "/api/cron-trigger",
      "schedule": "0 23 * * *"
    }
  ]
}